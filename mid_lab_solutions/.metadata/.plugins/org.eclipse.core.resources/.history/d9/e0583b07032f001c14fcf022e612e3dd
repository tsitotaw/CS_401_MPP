package enumconstantimp;

public class Constants {
	public static final String COMPANY = "Microsoft";
	public static final int SALES_TARGET = 20000000;
}

enum Constants_Enum {
	COMPANY("Microsoft"),
	SALES_TARGET(20000000);
	
	double val;
	String str;
	public Constants_Enum(String str) {
		this.str = str;
	}
	
	public Constants_Enum(double val) {
		this.val = val;
	}
	
}
