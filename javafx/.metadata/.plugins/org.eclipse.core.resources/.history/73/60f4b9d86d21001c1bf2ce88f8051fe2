package prob4;

public class Trailer {
	private double rent;
	
	public Trailer(double rent) {
		this.rent = rent;
	}
	
	public double computeRent() {
		return 500;
	}
	
	public double getRent() {
		return rent;
	}
	public void setRent(double rent) {
		this.rent = rent;
	}

	@Override
	public String toString() {
		return "Trailer [rent=" + rent + "]";
	}
}
