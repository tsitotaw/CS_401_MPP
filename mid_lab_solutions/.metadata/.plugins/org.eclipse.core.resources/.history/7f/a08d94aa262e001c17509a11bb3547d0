package genericmethods;

import java.util.List;

public class FindMax {

	public static void main(String[] args) {
		
	}
	
	public static Integer max(List<Integer> list) {
		Integer max = list.get(0);
		
		for(Integer i: list) {
			if(i.compareTo(max) > 0) {
				max = i;
			}
		}
		
		return max;
	}

}
