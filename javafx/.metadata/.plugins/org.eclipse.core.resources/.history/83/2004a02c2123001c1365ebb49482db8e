package part2;

public final class Triangle {
	
	private double width;
	private double height;	
		
	public Triangle(double width, double height) {
		super();
		this.width = width;
		this.height = height;
	}

	public double getWidth() {
		return width;
	}

	public double getHeight() {
		return height;
	}

	public double computeArea() {
		
		return 0.0;
	}	
}
